[run]
source = apps
omit =
    */migrations/*
    */tests.py
    */tests/*
    */__pycache__/*
    */management/commands/test_*.py
    */admin.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
